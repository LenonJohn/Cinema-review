<div class="container my-4">
  <div class="row">
    
    <div class="col-lg-2 sidebar_fixed">
      <%= render "search_form", q: @q, tag_search: @tag_search %>
      <%= render "tags/tag_list", tag_list: @tag_list %>
    </div>
    
    <div class="col-lg-6">
      
      <% @review.reverse_each do |review| %>
        <%= render "review", review: review %>
      <% end %>
      
    </div>
    
  </div>
</div>